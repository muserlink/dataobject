{
  "name": "dataobject",
  "version": "0.1.0",
  "description": "Data object decorator with marshalling utilities.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "rimraf dist && tsc -p tsconfig.build.json && node postbuild.js",
    "test": "jest --runInBand",
    "prettier": "prettier --ignore-path .gitignore --write \"src/**/*.{ts,js,json}\"",
    "eslint": "eslint --fix --ext .ts,.js src/",
    "format": "npm run prettier && npm run eslint"
  },
  "author": {
    "name": "Keisuke Yamamoto",
    "email": "keisuke@modelhouse.tech"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yamamotok/dataobject"
  },
  "keywords": [
    "typescript",
    "json",
    "marshaller",
    "transformer"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.18",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "dependencies": {}
}
